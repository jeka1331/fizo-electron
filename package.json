{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "server": "node server.js",
    "start": "electron-forge start",
    "build": "cd client && yarn build && cd .. && electron-builder",
    "buildwin": "cd client && yarn build && cd .. && electron-builder --win",
    "package": "cd client && yarn install --frozen-lockfile && yarn build && cd .. && npm ci && electron-forge package",
    "make": "cd client && yarn install --frozen-lockfile && yarn build && cd .. && npm ci && electron-forge make"
},
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "binary": {
    "napi_versions": [
      2,
      3
    ]
  },
  "build": {
    "appId": "kz.vnv.gizo",
    "productName": "Fiz Podgotovka",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "build/**/*",
      "server/**/*",
      "client/dist/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "npmRebuild": true
  },
  "dependencies": {
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "html-pdf": "^3.0.1",
    "p-limit": "^5.0.0",
    "pdfkit": "^0.14.0",
    "pug": "^3.0.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "electron": "^28.1.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
